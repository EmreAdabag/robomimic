{
    "algo_name": "diffusion_policy",
    "experiment": {
        "name": "debug_diffusion_mpc",
        "validate": false,
        "logging": {
            "terminal_output_to_txt": false,
            "log_tb": false
        },
        "save": {
            "enabled": true,
            "every_n_seconds": null,
            "every_n_epochs": 10,
            "epochs": [],
            "on_best_validation": false,
            "on_best_rollout_return": false,
            "on_best_rollout_success_rate": false
        },
        "epoch_every_n_steps": 100,
        "validation_epoch_every_n_steps": null,
        "rollout": {
            "enabled": false,
            "n": 10,
            "horizon": 200,
            "rate": 50,
            "warmstart": 0,
            "terminate_on_success": true
        }
    },
    "train": {
        "data": "datasets/lift/ph/low_dim_v15.hdf5",
        "output_dir": "./debug_diffusion_mpc_results",
        "num_data_workers": 0,
        "hdf5_cache_mode": "low_dim",
        "hdf5_use_swmr": true,
        "hdf5_normalize_obs": false,
        "hdf5_filter_key": null,
        "seq_length": 10,
        "pad_seq_length": true,
        "frame_stack": 1,
        "pad_frame_stack": true,
        "dataset_keys": [
            "actions",
            "rewards",
            "dones"
        ],
        "goal_mode": null,
        "cuda": true,
        "batch_size": 32,
        "num_epochs": 20,
        "seed": 1,
        "action_keys": [
            "actions"
        ],
        "action_config": {
            "actions": {
                "normalization": "min_max"
            }
        }
    },
    "algo": {
        "optim_params": {
            "policy": {
                "optimizer_type": "adamw",
                "learning_rate": {
                    "initial": 0.0001,
                    "decay_factor": 0.1,
                    "epoch_schedule": [],
                    "scheduler_type": "multistep"
                },
                "regularization": {
                    "L2": 0.000001
                }
            }
        },
        "horizon": {
            "observation_horizon": 2,
            "action_horizon": 4,
            "prediction_horizon": 8
        },
        "unet": {
            "enabled": true,
            "diffusion_step_embed_dim": 128,
            "down_dims": [128, 256, 512],
            "kernel_size": 3,
            "n_groups": 8
        },
        "mpc": {
            "enabled": true,
            "horizon": 10
        },
        "ema": {
            "enabled": true,
            "power": 0.75
        },
        "ddpm": {
            "enabled": true,
            "num_train_timesteps": 50,
            "num_inference_timesteps": 50,
            "beta_schedule": "squaredcos_cap_v2",
            "clip_sample": true,
            "prediction_type": "epsilon"
        },
        "ddim": {
            "enabled": false,
            "num_train_timesteps": 50,
            "num_inference_timesteps": 10,
            "beta_schedule": "squaredcos_cap_v2",
            "clip_sample": true,
            "set_alpha_to_one": false,
            "steps_offset": 0,
            "prediction_type": "epsilon"
        }
    },
    "observation": {
        "modalities": {
            "obs": {
                "low_dim": [
                    "robot0_joint_pos",
                    "robot0_joint_vel",
                    "robot0_gripper_qpos",
                    "object"
                ],
                "rgb": [],
                "depth": [],
                "scan": []
            },
            "goal": {
                "low_dim": [],
                "rgb": [],
                "depth": [],
                "scan": []
            }
        },
        "encoder": {
            "low_dim": {
                "core_class": null,
                "core_kwargs": {},
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            }
        }
    }
}
